<!DOCTYPE html>
<html>
<head>
  <title>WDI20FinalProject</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body id="homepage">

  <div id="wrapper">

    <!-- header begin -->
    <header class="header_center">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <!-- small button begin -->
            <span id="menu-btn"></span>
            <!-- small button close -->

            <!-- mainmenu begin -->
            <nav>
              <ul id="mainmenu">
                <li><%= link_to "Home", root_path %></li>
                <li><%= link_to 'Why?', page_path('why') %></li>
                <li><%= link_to 'Pricing', page_path('pricing') %></li>
                <li><%= link_to 'About', page_path('about') %></li>
                <li><a href="comments.html">Comments</a></li>
                <li>
                  <a href="gallery.html">User</a>
                  <ul>
                    <li><a href="gallery-2-cols.html">Login</a></li>
                    <li><a href="gallery-3-cols.html">Register</a></li>
                  </ul>
                </li>
              </ul>
            </nav>
            <div class="clearfix"></div>
          </div>
          <!-- mainmenu close -->

        </div>
      </div>
    </header>
    <!-- header close -->

    <!-- content begin -->
    <div id="content" class="no-bottom no-top">
      <%= yield %>
    </div>




    <!-- footer begin -->
    <footer>
      <div class="container">
        <div class="row">

          

          <div class="col-md-4">
            <div class="widget">
              <h3>We're Open</h3>
              <div class="box-border double">
                <ul class="list-border-bottom">
                  <li>
                    <span class="pull-left">Monday - Friday</span>
                    <span class="pull-right id-color">9 am to 10 pm</span>
                    <div class="clearfix"></div>
                  </li>
                  <li>
                    <span class="pull-left">Saturday</span>
                    <span class="pull-right id-color">9 am to 12 pm</span>
                    <div class="clearfix"></div>
                  </li>
                  <li>
                    <span class="pull-left">Sunday &amp; Holiday</span>
                    <span class="pull-right id-color">8 am to 12 pm</span>
                    <div class="clearfix"></div>
                  </li>
                </ul>
              </div>
            </div>
          </div>

          <div class="col-md-4 col-md-offset-4">
            <div class="widget widget-address">
              <h3>Contact Us</h3>

              <address>
                <span>100 Mainstreet Center, Sydney</span>
                <span><strong>Phone:</strong>(208) 333 9296</span>
                <span><strong>Fax:</strong>(208) 333 9298</span>
                <span><strong>Email:</strong><a href="mailto:contact@modis-salon.com">contact@modis-salon.com</a></span>
                <span><strong>Web:</strong><a href="#">http://modis-salon.com</a></span>
              </address>
            </div>
          </div>


        </div>
      </div>

      <div class="subfooter">
        <div class="container">
          <div class="row">
            <div class="col-md-6">
              &copy; Copyright 2016 - Modis by Designesia                     
            </div>
            <div class="col-md-6 text-right">
              <div class="social-icons">
                <a href="#"><i class="fa fa-facebook fa-lg"></i></a>
                <a href="#"><i class="fa fa-twitter fa-lg"></i></a>
                <a href="#"><i class="fa fa-rss fa-lg"></i></a>
                <a href="#"><i class="fa fa-google-plus fa-lg"></i></a>
                <a href="#"><i class="fa fa-skype fa-lg"></i></a>
                <a href="#"><i class="fa fa-dribbble fa-lg"></i></a>
              </div>
            </div>
          </div>
        </div>
      </div>

    </footer>
    <!-- footer close -->
  </div>



</body>
</html>
